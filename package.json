{
	"name": "@sveuikit/schema-builder",
	"version": "1.0.0",
	"description": "Json schema builder",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"lib": "vite build && npm run prepack",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && npx publint && npm publish --access=public",
		"prepublishOnly": "npm run prepack",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"package": "npm publish --access=public || bun publish"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"daisyui": "^5.0.3",
		"svelte": "^5.0.0",
		"tailwindcss": "^4.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.3",
		"@playwright/test": "^1.45.3",
		"@sveltejs/adapter-vercel": "^5.5.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@sveuikit/core": "^1.0.0",
		"autoprefixer": "^10.4.20",
		"daisyui": "^5.0.35",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"publint": "^0.2.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.9",
		"typescript": "^5.0.0",
		"vite": "^5.4.11",
		"vitest": "^2.0.4"
	},
	"dependencies": {
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"eventemitter3": "^5.0.1",
		"localforage": "^1.10.0",
		"nanoid": "^5.1.5",
		"sortablejs": "^1.15.6",
		"swapy": "^1.0.5",
		"zod": "^3.24.4"
	}
}
